{
  "scripts": {
    "dev": "concurrently \"cd client && npm run dev\" \"nodemon --watch server --exec node server/server.js\"",
    "start": "concurrently \"npm run start:back\" \"npm run start:front\"",
    "start:back": "nodemon server/server.js",
    "start:front": "cd client && npm run preview",
    "build": "cd client && npm run build"
  },
  "devDependencies": {
    "concurrently": "^8.2.2"
  }
}
